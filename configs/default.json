{
  "seed": 42,
  "device": "auto",
  "data_root": "./data",
  "data_num_workers": 0,
  "dataset": "traffic",
  "num_classes": 43,
  "model": "resnet18",
  "lambda_ib": 0.005,
  "lambda_contrast": 0.1,
  "lambda_consistency": 0.3,
  "lambda_distill": 0.05,
  "hdib_backbone": "resnet18",
  "backbone_pretrained": false,
  "rounds": 100,
  "num_shards": 2,
  "clients_per_shard": 5,
  "clients_per_round": 3,
  "local_epochs": 2,
  "batch_size": 64,
  "proto_batch_size": 128,
  "alpha_dirichlet": 0.5,
  "mal_ratio": 0,
  "lr": 0.005,
  "momentum": 0.9,
  "weight_decay": 0.0001,
  "max_grad_norm": 5.0,
  "muon_betas": [0.95, 0.999],
  "muon_eps": 1e-8,
  "use_reputation_consensus": false,
  "wasserstein_threshold": 0.8,
  "init_reputation": 0.8,
  "use_blockchain_memory": false,
  "history": 4,
  "ema_decay": 0.7,
  "use_cluster_prototypes": false,
  "cluster_threshold": 0.85,
  "use_quality_fusion": false,
  "alpha_quality": 0.6,
  "beta_quality": 0.2,
  "gamma_quality": 0.2
}
