{
  "seed": 42,
  "device": "auto",
  "data_root": "./data",
  "num_classes": 10,
  "model": "hdib",
  "rounds": 30,
  "num_shards": 2,
  "clients_per_shard": 5,
  "clients_per_round": 2,
  "local_epochs": 3,
  "batch_size": 64,
  "proto_batch_size": 128,
  "alpha_dirichlet": 0.5,
  "lr": 0.01,
  "momentum": 0.9,
  "weight_decay": 0.0001,
  "max_grad_norm": 5.0,
  "muon_betas": [0.9, 0.99],
  "muon_eps": 1e-8,
  "init_reputation": 0.8,
  "history": 4,
  "ema_decay": 0.7,
  "use_cluster_prototypes": true,
  "cluster_threshold": 0.85,
  "use_quality_fusion": true,
  "use_reputation_consensus": false,
  "use_blockchain_memory": false,
  "lambda_ib": 1.0,
  "lambda_contrast": 0.2,
  "lambda_consistency": 0.5,
  "lambda_distill": 0.1,
  "wasserstein_threshold": 0.8,
  "alpha_quality": 0.6,
  "beta_quality": 0.2,
  "gamma_quality": 0.2
}
